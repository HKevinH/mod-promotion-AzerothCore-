#######################################################
### AzerothCore Module Promotion configuration file ###
#######################################################
[worldserver]

###################################################################################################
# SECTION INDEX
#
#    ANNOUNCE ENABLE
#    SETTIGNS MODULE
#    CLASS CONFIG
#
###################################################################################################

###################################################################################################
# 
# ANNOUNCE ENABLE
# Enable Hello World message at server start 
# default: true
#
#
announce_module.enableHelloWorld=true

######################################################################################################
#	SETTIGNS MODULE
#
#		NpcPromotion.enable
#
#			Description:	Variable that enables or disables the module
#
#			Example:	"true" - (Enabled)
#						"false" - (Disabled)
#
#        Default:     true

promotionEnable.enable=true

#
#
#	    Promotion.count
#
#		Description:	Amount of promotion per account.
#
#			Example:	1 - (One)
#						2 - (Two)
#
#        Default:     1

promotion.count = 1

#
#
#     MoneyRewardPromotion
#
#     Description: Amount of promotion for character
#
#     Example: 10 - (10 cobber)
#              25000000 - (2500 gold)
#
#     Default: 25000000
#

MoneyRewardValue = 25000000

#
#
#     Level Promotion
#
#     Description: Now you can set the level you want for the promotion
#
#     Example: 80 - ( Character Level )
#              70 - ( Character Level )
#
#     Default: LevelForPromotion = 80
#

LevelForPromotion = 80

#
#
#     MountPromotion
#
#     Description: Here You Put The Spells Of The Promotion Mounts
#
#     Example: 42277 - ( Swift Spectral Tiger - ID SPELL)
#              37015 - ( Swift Nether Drake - ID SPELL)
#
#     Default: mountPromotion = 42277
#     Default: mountEnable.enable = true (Mounts Enabled In The Promotion)
#

mountEnable.enable = true
mountPromotion = 42277

#
#
#   Bags Enable
#
#   Description: Configuration for the backpacks in the promotion
#   
#   Default: bagEnable.enable = true (enabled backpacks promotion)
#   Default: bagReward.Id = 14156 Bottomless Bag Binds when equipped 18 Slot Bag)
#
#

bagEnable.enable = true
bagReward.Id = 41599

